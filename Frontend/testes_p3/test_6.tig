/* buble sort - array with 10 integer */

let
    type arrayt = array of int
/* baseado no ex 29 do site do livro */
	var vetor: arrayt := arrayt [10] of 0 
	var tam:= 10

    function bublesort(tam: int, vetor: arrayt) =
    let
        var aux := 0
    in
        for i := 0 to (tam-1) do (
            for j := 0 to ((tam-i)-1) do (
                if vetor[j] > vetor[j+1] then
                    aux := vetor[j];
                    vetor[j] := vetor[j+1];
                    vetor[j+1] := aux;
                );
        );
    end
in
    bublesort(tam, vetor)
end