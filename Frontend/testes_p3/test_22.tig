/* write a program using your new construction, such code must have 30 lines and also print something */
let
	var a:= 1
	var b:= 2
	var c:= 3
	var d:= 0
in

	switch a {
		case 1:
			a := b+c
            b := c+d
		case 2:
			a := a+c
            b := a
		default:
			b := a+d
	} 
	switch b {
		case 1:
			a := b+c
            b := a+b
		case 2:
			a := a+c
            c := a-d
        case 3:
            a := a+c
            b := a-c
    	case 4:
			a := a+c
            d := a+a
        case 5:
			a := a+c
            c := (c+b+d)-a
		default:
			a := a+d
	} 
    printint(a)
    printint(b)
    printint(c)
    printint(d)
end
